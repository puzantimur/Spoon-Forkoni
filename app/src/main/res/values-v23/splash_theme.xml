<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.AppSplash" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@color/green</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/lovepik_com_401037441_cook</item>
        <item name="windowSplashScreenAnimationDuration">300</item>
        <item name="postSplashScreenTheme">@style/Theme.HomeW3</item>

        <!-- Status bar and Nav bar configs -->
        <item name="android:statusBarColor">@color/black</item>
        <item name="android:navigationBarColor">@color/black</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>
</resources>